    input {
      font-family: {{ settings.button_font }};
      font-size: {{ settings.button_font_size }};
      {% if settings.button_font_transform %}
      text-transform: uppercase;
      {% endif %}
      
      border-radius:5px;
      border:solid 1px {{ settings.color_borders }};
      background-color:{{ settings.color_input_bg }};
      color:{{ settings.color_input_text }};
      max-width:100%;
      margin:0.1em 1em 0.5em 0em;

    }
    input[type='number']{
      text-align:center;
    }

    input{
      padding:0.5em 0.5em 0.5em 0.5em;
    }

    select{
      padding:0.3em 0.5em 0.3em 0.5em;  
    }
    label
    {
      margin:0.8em 0.5em 0.8em 0em;	  
      display:inline-block;
      text-align:left;
    }
    .labeltitle
    {
      font-size: {{ settings.label_font_size }};
      font-family:{{ settings.button_font }}; 
      color:{{ settings.color_title_text }};
      margin-left:0.1em;
    }
    .labellogin
    {
      font-size: {{ settings.label_font_size }};
      font-family:{{ settings.button_font }}; 
      color:{{ settings.color_title_text }};
      margin-left:0.1em;
    }
    .labellogin input
    {
      text-transform: none;
    }
    hr{
      background-color:{{ settings.color_borders }};
      display: block;
      margin-top: 3em;
      margin-bottom: 3em;
      color:{{ settings.color_borders}};
      border: none;
	  height:1px;
    }

    Button{
      background-color:{{ settings.color_button_bg }};
      border:solid 1px {{ settings.color_button_border }};
      color:{{ settings.color_button_text }};
      font-family: {{ settings.button_font }};
      font-size: {{ settings.button_font_size }};
      {% if settings.button_font_transform %}
      text-transform: uppercase;
      {% endif %}
      padding:0.5em 0.8em 0.5em 0.8em;
      border-radius:5px;
      margin:1em 1em 1em 0em;
    }
    Button:disabled{
      background-color:{{ settings.color_button_disabled }};
    }
    .h1center{
      text-align:center;
      margin-top:2em;
      margin-bottom:2em;
    }
    .h2center{
      text-align:center;
      margin-top:1em;
      margin-bottom:1em;
    }  
    .nodecoration{
      text-decoration: none;
    }
    .marginauto{
      margin:auto;
    }
    .nostyle{
      list-style: none;
    }
    .posbottom{
      position: absolute;
      bottom:0;
    }
    .container{
      width:100%;
    }
    .footbar{
      text-align:center;
      margin-top: 3em;
      border-top: 1px solid {{ settings.color_borders }};
      padding-top:6em;
      background-color:{{ settings.color_footer_bg }};
      text-align:center;
      line-height:{{ settings.footer_font_size }};
      font-size: {{ settings.footer_font_size }};
      font-family:{{ settings.footer_font }};  
    }
    .footbar p{
      color:{{ settings.color_footer_text }};
      font-family:{{ settings.footer_font }};
    }
    .footbar a{
      color:{{ settings.color_footer_links }};
      font-family:{{ settings.footer_font }};
    }
    .footbar hr{
      background-color:{{ settings.color_footer_links }};
      display: block;
      margin-top: 3em;
      margin-bottom: 3em;
      color:{{ settings.color_footer_links}};
      border: none;
	  height:1px;
    }
