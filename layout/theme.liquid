    .margin-15pert{
      margin-top: 15%;
      margin-bottom: 15%;
    }
    .margin-20pert{
      margin-top: 20%;
      margin-bottom: 20%;
    }

    .fixed-height-container {
     position: relative;
     width: 100%;
     height: 300px;
    }
    .margin-1em{
      margin-top:1em;
      margin-bottom:1em;
    }
    .margin-2em{
      margin:2em;
    }
    
    .float-left{
      float:left;
    }
    .float-right{
      float:right;
    }
    .full-padding-1em{
      padding:1em;
    }
    .text-align-center{
      text-align:center;
    }
    .text-align-left{
      text-align:left;
    }
    .text-align-right{
      text-align:right;
    }
